<?xml version="1.0" encoding="UTF-8"?>
<converters>
	<converter>
		<metadata-prefix>mets,mets_exp</metadata-prefix>
		<label>original</label>
		<urlsuffix>Content/${id}/${filename}</urlsuffix>
	</converter>
	<converter>
	<label>f3d</label>
		<metadata-prefix>none</metadata-prefix>
		<inputmimetype>image/xml</inputmimetype>
		<outputmimetype>image/xml</outputmimetype>
		<prefix>f3d</prefix>
		<urlsuffix>Content/${id}/f3d/3DData.xml</urlsuffix>
	</converter>
	<converter>
		<metadata-prefix>mets_exp</metadata-prefix>
		<label>thumbnail</label>
		<inputmimetype>image/x.djvu</inputmimetype>
		<outputmimetype>image/jpeg</outputmimetype>
		<parameters>20%,0.75</parameters>
		<prefix>d2j</prefix>
		<urlsuffix>Content/${id}/d2j:thumbnail,${page}/${filename}.jpg</urlsuffix>
	</converter>
	<converter>
		<label>preview</label>
		<metadata-prefix>mets_exp</metadata-prefix>
		<inputmimetype>image/x.djvu</inputmimetype>
		<outputmimetype>image/jpeg</outputmimetype>
		<parameters>70%,0.75</parameters>
		<prefix>d2j</prefix>
		<urlsuffix>Content/${id}/d2j:preview,${page}/${filename}.jpg</urlsuffix>
	</converter>
	<converter>
		<label>big</label>
		<metadata-prefix>mets_exp</metadata-prefix>
		<inputmimetype>image/x.djvu</inputmimetype>
		<outputmimetype>image/jpeg</outputmimetype>
		<parameters>100%,0.75</parameters>
		<prefix>d2j</prefix>
		<urlsuffix>Content/${id}/d2j:big,${page}/${filename}.jpg</urlsuffix>
	</converter>
	<converter>
		<label>jpg_thumbnail</label>
		<metadata-prefix>mets_exp</metadata-prefix>
		<inputmimetype>image/jpeg,text/xml,image/gif,image/png</inputmimetype>
		<outputmimetype>image/jpeg</outputmimetype>
		<parameters>300,300,0.75</parameters>
		<prefix>i2j</prefix>
		<urlsuffix>Content/${id}/i2j:jpg_thumbnail/${filename}.jpg</urlsuffix>
	</converter>
	<converter>
		<label>zip</label>
		<metadata-prefix>none</metadata-prefix>
		<outputmimetype>application/zip</outputmimetype>
		<prefix>zip</prefix>
	</converter>
</converters>